{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "noImplicitUseStrict": false,
    "strictNullChecks": false,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "modules.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/components/legacy/Orphanarium/InstructionalPopup.js", "src/components/legacy/Orphanarium/components/AudioplayerPopup/index.jsx"],
  "exclude": ["node_modules", "__src"]
}
